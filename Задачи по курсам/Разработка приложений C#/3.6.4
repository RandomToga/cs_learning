/* 3.6.4
Напишите программу, которая рассчитывает и выводит сумму нечётных элементов целочисленного массива и произведение чётных.*/

using System;

public class MainClass
{
    public static void Main()
    {
        int n = Convert.ToInt32(Console.ReadLine());
        int prod = 1;
        int su = 0;
        int[] array = new int[n];
        for(int i=0; i<n; i++){
            array[i] = Convert.ToInt32(Console.ReadLine());
            if(array[i]%2==0) prod*=array[i];
            else su+=array[i];
        }
        if(su==0) Console.WriteLine("Нечётных чисел нет!");
        else Console.WriteLine("Сумма нечётных = "+su);
        if(prod==1) Console.WriteLine("Чётных чисел нет!");
        else Console.WriteLine("Произведение чётных = "+prod);
    }
}

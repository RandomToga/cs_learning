/* 3.6.5
Напишите программу, которая выведет перевёрнутый целочисленный массив (элементы с последнего по первый).*/

using System;

public class MainClass
{
    public static void Main()
    {
        int n = Convert.ToInt32(Console.ReadLine());
        int[] array = new int[n];
        for(int i=0; i<n; i++) array[i] = Convert.ToInt32(Console.ReadLine());
        for(int i=n-1; i>=0; i--) Console.WriteLine(array[i]);
    }
}

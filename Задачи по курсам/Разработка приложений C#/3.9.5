/* 3.9.5
Напишите программу, которая считает сумму элементов, кратных 5, в целочисленном одномерном массиве. В решении задачи объявите, реализуйте, а затем используйте функцию с названием Sum_of_multiplicity5.*/

using System;

class Program
{
    static void Sum_of_multiplicity5(int n, int[] array){
        int su = 0;
        for (int i=0; i<n; i++) if (array[i]%5==0) su+=array[i];
        if (su==0) Console.WriteLine("Элементы кратные 5 отсутствуют");
        else Console.WriteLine("Сумма элементов кратных 5 = "+su);
    }
    static void Main()
    {
        int n = Convert.ToInt32(Console.ReadLine());
        string[] arrtemp = Console.ReadLine().Split(' ');
        int[] array = new int[n];
        for (int i=0; i<n; i++) array[i] = Convert.ToInt32(arrtemp[i]);
        Sum_of_multiplicity5(n, array);
    }
    
}

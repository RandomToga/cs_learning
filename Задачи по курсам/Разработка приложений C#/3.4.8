/* 3.4.8
Напишите программу, которая выводит сумму всех чётных чисел в интервале от 0 до N включительно, за исключением 4, 6, 8.*/

using System;

public class MainClass
{
    public static void Main()
    {
        int n = Convert.ToInt32(Console.ReadLine());
        int result = 0;
        for(int i=0; i<=n; i+=2){
            if(i==4 || i==6 || i==8) continue;
            else result+=i;
        }
        Console.WriteLine("Сумма чётных чисел = " + result);
    }
}
